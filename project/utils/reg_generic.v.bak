module register #(parameter WIDTH=32)(
	input [WIDTH-1: 0] data,
	input clk,
	input nreset,
	input ena,
	input [WIDTH-1: 0] out,
 );

	reg[31:0] q;
	wire reset;
	
	//assign reset = ~nreset;
 
	always @(posedge clk, negedge nreset) begin
		if (!nreset) begin
			q <= 0;
		end
		else if (clk && ena) begin
			q <= d;
		end
	end

	assign out = q;


endmodule